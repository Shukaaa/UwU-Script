# Loops

i.init(str<i>)
loop.while(math.less(i.get(str<i>), int<10>))
    ...
    i.inc(str<i>)
loop.end()

i.init(str<i>)
loop.repeat(arr.length(str<arr>))
    ...
    i.inc(str<i>)
loop.end()

loop.continue()
loop.break()


