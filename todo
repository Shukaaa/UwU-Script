console.ask(message)

# Arrays
array.create(name, type)
array.count(name)
array.pos(name, value)
array.add(name, value)
array.rem(name, index)
array.get(name, index)

# If Else

logic.if(statement)
...
logic.if_ends()


logic.if_else(statement)
...
logic.elseif(statement, elseAmounts)
...
logic.else(statement, elseAmounts)
...
logic.if_else_ends()


# Loops
loop.while()
loop.for()
loop.for_each()

# Function
func.define(name, func.param(type, optional_default_value))
func.end()


