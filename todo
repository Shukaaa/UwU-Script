# String operator
str.append(str1, str2) // trying to add infinity parameters
str.length(str)
str.yukihara(str, delimiter) // output array with strings (cutting the string like soma yukihara)
str.startWith()
str.endsWith()

# Array Sorts
array.sort(name, sort_type)
array.sort(name, sort_type, sort_order)

# If Else
cs.elseif(statement)

# Loops
cs.while()
cs.for()
cs.for_each()
cs.end()

# Function
func.define(name, func.param(type, optional_default_value))
func.end()

